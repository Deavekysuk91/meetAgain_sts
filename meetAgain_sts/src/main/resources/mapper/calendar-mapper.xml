<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="calendarMapper">

	<!-- <resultMap type="Calendar" id="">
		<id></id>
		<result> </result>
		<result> </result>		
	</resultMap> -->	
	
	<insert id="insertSchedule" parameterType="Calendar">
		INSERT INTO CALENDAR VALUES(#{gid}, #{gdate}, #{gdateEnd}, #{ginfo}, #{gtime}, DEFAULT)
	</insert>
	
	<select id="loadList" resultType="Calendar">
		<!-- SELECT C.*, SUBSTR((TO_CHAR(GTIME, 'HH:MI'), 1, 11) FROM CALENDAR C -->
		SELECT GID, GDATE, GDATEEND + 1 "GDATEEND", GINFO, GTIME, ISCTN FROM CALENDAR
	</select>
	
</mapper>