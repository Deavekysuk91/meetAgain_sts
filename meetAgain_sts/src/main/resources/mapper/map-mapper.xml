<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapMapper">

	<select id="createMapList" parameterType="string" resultType="Sgroup">
		SELECT GID, GPLACE FROM SGROUP WHERE GID = #{gid}
	</select>
	
	<select id="meetingPlaceMasterStatus" parameterType="string" resultType="Joing" >
		SELECT GID, ISCPT FROM JOING WHERE GID = #{gid}
	</select>
	
</mapper>